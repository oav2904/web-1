{
    "name": "crojasaragonez/utn-db",
    "type": "library",
    "description": "Simple database access package for UTN students",
    "keywords": [
        "crojasaragonez",
        "UtnDb"
    ],
    "homepage": "https://github.com/crojasaragonez/utn-db",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Luis Rojas AragonÃ©s",
            "email": "crojas@golabstech.com",
            "homepage": "https://github.com/crojasaragonez",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0.10"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "crojasaragonez\\UtnDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "crojasaragonez\\UtnDb\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
